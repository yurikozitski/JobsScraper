{
  "Djinni": {
    "Domain": "https://djinni.co/jobs/",
    "ShortDomain": "https://djinni.co",
    "XPaths": {
      "VacancyList": "//li[contains(@id, 'job-item')]",
      "LocalLink": ".//a[@class = 'job-item__title-link']",
      "JobTitle": ".//a[@class = 'job-item__title-link']",
      "JobType": ".//div[@class = 'fw-medium d-flex flex-wrap align-items-center gap-1']",
      "Company": ".//a[@class = 'text-body']",
      "PublicationDate": ".//span[@data-original-title]",
      "Salary": ".//span[@class = 'public-salary-item']",
      "Location": ".//span[@class = 'location-text']",
      "Description": ".//span[contains(@id, 'job-description')]",
      "Pagination": "//ul[@class = 'pagination pagination_with_numbers']"
    }
  },
  "DOU": {
    "Domain": "https://jobs.dou.ua/vacancies/",
    "XPaths": {
      "LoadMoreButton": "div[class='more-btn']",
      "VacancyList": "//li[@class = 'l-vacancy']",
      "LinkTitle": ".//a[@class = 'vt']",
      "JobTypeLocation": ".//span[@class = 'cities']",
      "Company": ".//a[@class = 'company']",
      "PublicationDate": ".//div[@class = 'date']",
      "Salary": ".//span[@class = 'salary']",
      "Description": ".//div[@class = 'sh-info']"
    }
  },
  "RobotaUa": {
    "Domain": "https://robota.ua/zapros/",
    "ShortDomain": "https://robota.ua",
    "XPaths": {
      "VacancyListParent": "//alliance-jobseeker-desktop-vacancies-list",
      "VacancyList": "//alliance-vacancy-card-desktop",
      "JobTitle": ".//h2",
      "Pagination": "//div[contains(@class, 'paginator')]"
    }
  },
  "Recruitika": {
    "Domain": "https://recruitika.com/",
    "ShortDomain": "https://recruitika.com",
    "XPaths": {
      "VacancyList": "//div[@class = 'vacancy-row']",
      "JobTitle": ".//div[@class = 'vacancy-title']",
      "JobType": ".//ul[@class = 'vacancy-tags-list']",
      "Company": ".//a[@class = 'vacancy-info__company']",
      "PublicationDate": ".//div[@class = 'vacancy-col vacancy-col--date']",
      "Salary": ".//span[@class = 'vacancy-info__salary']",
      "Location": ".//span[@class = 'vacancy-info__city']",
      "Pagination": "//div[@class = 'paginate-links-wrapper']"
    }
  }
}